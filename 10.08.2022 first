
int main() 
{
   int n,r,i=0,j=1;
   printf("enter any number");
   scanf("%d",&n);
   int x,y;
   printf("enter x and y values");
   scanf("%d %d",&x,&y);
   while(n>0)//1691
   {
       r=n%10;//r=9
       if(r==x)
       i=i+y*j;//i=87
       else
       i=i+r*j;//i=
       j=j*10;//j=100
       n=n/10;//169
       
   }
   printf("%d",i);

    return 0;
}
